{
  "configurations": {
    "frontend:launch": {
      "adapter": "chrome",
      "configuration": {
        "request": "launch",
        "runtimeExecutable": "/usr/bin/google-chrome-stable",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceRoot}"
      },
      "breakpoints": {
        "exception": {
          "all": "N",
          "uncaught": "N"
        }
      }
    },
    "frontend:attach": {
      "adapter": "chrome",
      "configuration": {
        "request": "attach",
        "urlFilter": "http://localhost:3000/*",
        "port": 9230,
        "webRoot": "${workspaceRoot}/apps/frontend"
      },
      "breakpoints": {
        "exception": {
          "all": "N",
          "uncaught": "N"
        }
      }
    }
  }
}
